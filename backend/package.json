{
  "name": "recipe_prep-backend",
  "version": "1.0.0",
  "description": "Project for CSE 115A",
  "author": "Tyler Hayden, Adam Barsness, Emmanuel Pena, Shane Smith, Joshua Harding ",
  "license": "UNLICENSED",
  "repository": "https://git.ucsc.edu/jorhardi/cse-115a-meal-prep",
  "devDependencies": {
    "eslint": "*",
    "eslint-config-google": "*",
    "jest": "*",
    "nodemailer-mock": "^1.5.11",
    "nodemon": "*",
    "supertest": "*"
  },
  "dependencies": {
    "bcrypt": "*",
    "body-parser": "*",
    "cors": "*",
    "dotenv": "*",
    "express": "*",
    "express-openapi-validator": "*",
    "express-session": "*",
    "google-auth-library": "^7.11.0",
    "googleapis": "^95.0.0",
    "handlebars": "^4.7.7",
    "js-yaml": "*",
    "jsonwebtoken": "*",
    "nodemailer": "^6.7.2",
    "pg": "*",
    "swagger-ui-express": "*"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/server.js"
    ],
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  },
  "eslintConfig": {
    "extends": [
      "google"
    ],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "indent": [
        "warn",
        2
      ]
    }
  },
  "scripts": {
    "start": "nodemon src/server.js",
    "test": "jest --runInBand --coverage --verbose --forceExit",
    "lint": "./node_modules/.bin/eslint . --ext .js"
  }
}
